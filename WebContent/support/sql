create table ssabal(
onenumber varchar2(50),
accounts varchar2(50),
ids varchar2(50),
donation int,
mof varchar2(1),
attrs varchar2(1),
constraint donation_fk FOREIGN key (ids) references ORDINARY_PEOPLE(op_id)
);
바로 위에 있는게 내꺼 구동에 필요한 테이블 생성입니다


drop table ssabal;
/
desc ssabal;
/
select * from ssabal;
/



CREATE TABLE ORDINARY_PEOPLE(
OP_ID VARCHAR2(50),
OP_NAME VARCHAR(5) NOT NULL,
OP_PW VARCHAR2(25) NOT NULL,
OP_BDATE VARCHAR2(6) NOT NULL,
OP_EMAIL VARCHAR2(25) NOT NULL,
OP_ADDR VARCHAR2(100) NOT NULL,
OP_PCODE INT NOT NULL,
OP_GENDER VARCHAR2(1) NOT NULL,
OP_PHONE INT NOT NULL,
OP_DATE DATE,
PRIMARY KEY(OP_ID));
/
CREATE TABLE ORGANIZATION(
OR_LINUMBER VARCHAR2(50) NOT NULL,
OR_MAN VARCHAR2(50) NOT NULL,
OR_NAME VARCHAR(5) NOT NULL,
OR_ID VARCHAR2(50)NOT NULL,
OR_PW VARCHAR2(25) NOT NULL,
OR_PHONE INT NOT NULL,
OR_ADDR VARCHAR2(100) NOT NULL,
OR_PCODE INT NOT NULL,
OR_EMAIL VARCHAR2(25) NOT NULL,
OR_DATE DATE,
PRIMARY KEY(OR_LINUMBER));
/

CREATE TABLE CORPORATION(
CR_NUMBER VARCHAR2(50) NOT NULL,
CR_NAME VARCHAR(5) NOT NULL,
CR_ID VARCHAR2(50)NOT NULL,
CR_PW VARCHAR2(25) NOT NULL,
CR_PHONE INT NOT NULL,
CR_ADDR VARCHAR2(100) NOT NULL,
CR_PCODE INT NOT NULL,
CR_MAN VARCHAR2(50) NOT NULL,
CR_EMAIL VARCHAR2(25) NOT NULL,
CR_DATE DATE,
PRIMARY KEY(CR_NUMBER));create table ssabal(
onenumber varchar2(50),
accounts varchar2(50),
ids varchar2(50) PRIMARY key,
donation int,
mof varchar2(1),
attrs varchar2(1)
constraint donation_fk FOREIGN key (ids) references ORDINARY_PEOPLE(op_id)
);

drop table ORDINARY_PEOPLE;
